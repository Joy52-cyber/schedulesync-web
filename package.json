{
  "name": "schedulesync-web",
  "version": "1.0.0",
  "description": "Modern scheduling application",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
